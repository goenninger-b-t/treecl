(print "Verifying User Case: frgo")
(defun frgo (x) (* x x))

(print "Symbol structure:")
(print (tree-string 'frgo)) 
;; Expected: "n" because symbol is a leaf

(print "Closure structure (uncompiled):")
(print (tree-string #'frgo))
;; Expected: "n" because closure is an opaque leaf

(print "Compiled structure:")
(print (tree-string (compile 'frgo)))
;; Expected: meaningful tree structure e.g. (n ((n ...) ...))
