;; Basic Arithmetic
(+ 1 2) 
;; EXPECT: 3

(* 3 4)
;; EXPECT: 12

(- 10 3)
;; EXPECT: 7

;; Float Arithmetic
(+ 1.5 2.5)
;; EXPECT: 4

(* 2.0 2.5)
;; EXPECT: 5

;; Booleans (Prelude)
(if true 1 2)
;; EXPECT: 1

(if false 1 2)
;; EXPECT: 2

;; Primitives
(inc 10)
;; EXPECT: 11

(dec 10)
;; EXPECT: 9

(abs -5)
;; EXPECT: 5
