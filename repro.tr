
(def concat
  (fn l1
      (fn l2
        (matchList l2
                   (fn h (fn t (cons h (concat l1 t))))
                   l1))))

(def filter
  (fn pred
    (fn coll
      (reduce
        (fn acc 
          (fn e
            (if (pred e)
              (concat acc (cons e n))
              acc)))
        n 
        coll))))

(take 5 (filter (fn e (= 0 0))
                (range 0 100)))
