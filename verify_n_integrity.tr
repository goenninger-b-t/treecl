;; Verify 'n' (Leaf) Integrity
;; n should be Leaf (n)

;; 1. Direct value
n
;; EXPECT: n

;; 2. Primitives on n
(first n)
;; EXPECT: n

(rest n)
;; EXPECT: n

;; 3. List construction
(cons 1 n)
;; EXPECT: (1)

(cons n n)
;; EXPECT: (n)

;; 4. K combinator check (n n)
(def k (cons n n))
k
;; EXPECT: (n)

(first k)
;; EXPECT: n

(rest k)
;; EXPECT: n

;; 5. Deep n
(cons 1 (cons 2 (cons 3 n)))
;; EXPECT: (1 2 3)
